  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "debug": "tsc --noEmit",
    "start": "node dist/index.js",
    "dev": "nodemon -e ts --exec \"ts-node -r tsconfig-paths/register ./src/app.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run -d src/data/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/data/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d src/data/data-source.ts -n"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.12",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
